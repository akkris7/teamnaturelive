<ul class="pagination">
    {% unless paginate.previous.is_link %}
      <li class="page-item disabled">
        <span class="page-link"><i class="fa fa-long-arrow-left" aria-hidden="true"></i></span>
      </li>
    {% else %}
    <li class="page-item">
      <a href="{{ paginate.previous.url }}"  class="page-link">
        <span class=""><i class="fa fa-long-arrow-left" aria-hidden="true"></i></span>
      </a>
    </li>
    {% endunless %}
 
    {% for part in paginate.parts %}
    {% if part.is_link %}
      <li class="page-item">
        <a class="page-link" href="{{ part.url }}" title="">{{ part.title }}</a>
      </li>
    {% else %}
      {% if part.title == paginate.current_page %}
        <li class="page-item active"><span class="page-link">{{ part.title }}</span></li>
      {% else %}
        <li class="page-item"><span class="page-link">{{ part.title }}</span></li>
      {% endif %}
    {% endif %}
  {% endfor %}
 
    {% unless paginate.next.is_link %}
   <li class="page-item disabled">
        <span class="page-link"><i class="fa fa-long-arrow-right" aria-hidden="true"></i></span>
     </li>
    {% else %}
    <li class="page-item">
      <a href="{{ paginate.next.url }}"  class="page-link">
        <span class="icon__fallback-text"><i class="fa fa-long-arrow-right" aria-hidden="true"></i></span>
      </a>
    </li>
    {% endunless %}
</ul>
