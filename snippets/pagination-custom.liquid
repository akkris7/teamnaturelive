<div class="toolbar-bottom">
  <ul class="pagination a-center">
    {% if paginate.previous %}
    <li class="page-item prev"><a class="page-link" href="{{ paginate.previous.url }}" title="{{ paginate.previous.title }}"><i class="fa fa-angle-left" ></i></a></li>
    {% else %}
    <li class="page-item disabled"><span class="page-link"><i class="fa fa-angle-left" ></i> </span></li>
    {% endif %}

    {% for part in paginate.parts %}
    {% if part.is_link %}
    <li class="page-item">
      <a class="page-link" href="{{ part.url }}" title="">{{ part.title }}</a>
    </li>
    {% else %}
    {% if part.title == paginate.current_page %}
    <li class="page-item active"><span class="page-link">{{ part.title }}</span></li>
    {% else %}
    <li class="page-item"><span class="page-link">{{ part.title }}</span></li>
    {% endif %}
    {% endif %}
    {% endfor %}

    {% if paginate.next %}
    <li class="page-item next"><a class="page-link" href="{{ paginate.next.url }}" title="{{ paginate.next.title }}"><i class="fa fa-angle-right" ></i></a></li>
    {% else %}
    <li class="page-item disabled"><span class="page-link"><i class="fa fa-angle-right" ></i></li>
      {% endif %}
  </ul>
</div>



