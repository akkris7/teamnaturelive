      float: $left;
      width: 25%;
      @include transition(all 0.3s ease); 
      @media(max-width: 991px) {
        width: 50%;
      }
       @media(max-width: 767px) {
        width: 100%;
      }
      &:hover{
        background: #222;
        .service-ico{
          .image{
            img{
                  transform: scale(0.8);
            }
          }
        }
        .service-info{
         h2{
            a{
              color: #fff;
            }
          }
          .des{
            color: #fff;
          }
        }
      }
    }
    .services-inline{
      cursor: pointer; 
      text-align: center;
      position: relative;
      @media(max-width: 767px) {
        >div{
          text-align: $left; 
          .service-ico{
            min-width: 47px;
            text-align: center;
          }
        }
      }
      @media(max-width: 567px) {
        text-align: center;; 
      }
      .policy.policy4{
        &:before {
          display: none; 
        }
      }
      .service-ico{
        margin-top: -5px;
        float: $left;
        position: relative;
        line-height: 100%;
        color: #222;
        position: relative;
        padding-#{$left}: 35px;
        i{font-size: 42px;}
        .image{
            img{
              @include transition(all 0.3s ease); 
            }
        }
        @media(max-width: 1200px) and (min-width: 767px){
          display: block; 
          top: auto;
          i{font-size: 36px;}
        }
      }
      .service-info{
        margin-#{$left}: 100px;
        text-align: $left;

        h2{
          @include transition(all 0.3s ease); 
          font-weight: 400;
          margin: 0;
          color: #222222;
          font-size: 14px;
          a{
            color: #222222;
            @include transition(all 0.3s ease); 
          }
          
          @media(max-width: 567px) {
            padding-top: 5px;
          }
        }
        p{
          text-transform: capitalize;
    		font-size: 13px;
          color: #666666;
          margin-top: 5px;
          margin-bottom: 0;
          @include transition(all 0.3s ease); 
        }
   
      }

    }
  }
}

.block-sidebar{
  margin-top: 30px;
  .home-title{

    line-height: 100%;
    margin: 0;
    padding-bottom: 10px;
    border-bottom: 3px solid #f4f4f4;
    position: relative;
    overflow: visible;
    margin-bottom: 20px;
    &:before {
      position: absolute;
      bottom: -3px;
      left: 0;
      width: 70px;
      height: 3px;
      content: "";
      background-color: $main-color;
    }
    span {
      font-size: 18px;
      text-transform: uppercase;
      font-weight: bold;
      color: #000;
    }
  }
  .product-thumb .product-item {
    margin-bottom: 20px;
    clear: both; 
    overflow: hidden;
    .product-img{
      @include transition(all 0.3s ease); 
      width: 85px;
      max-width: 100%;
      float: $left;
      margin:side-values( 0 15px 0 0);
      &:hover {
        box-shadow: 5px 5px 0 #e5e5e5;
      }
    }
    .product-info{
      padding-bottom: 10px;
      margin-#{$left}: 100px;
      background: transparent;
      color: #666;
      border-bottom: 1px dotted #bdbdbd;
      position: relative;
      .product-name{
        display: inline-block;
        font-weight: 400;
        color: #666;
        line-height: 100%;
        padding: 0;
        &:hover{
          color: $main-color; 
        }
      }
      .price{
        margin: 2px 0 0; 
      }
      .box-price .price-new, .box-price span.price, .price .price-new, .price span.price{
        font-size: 16px;
      }
    }
  } 
  &.tags a {
    float: $left;
    line-height: 100%;
    background-color: rgb(255, 255, 255);
